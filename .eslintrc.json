{
  "extends": ["next", "prettier"],
  "rules": {
    "react/react-in-jsx-scope": 0,
    "react/display-name": 0,
    "react/prop-types": 0,
    "explicit-function-return-type": 0,
    "explicit-member-accessibility": 0,
    "indent": 0,
    "member-delimiter-style": 0,
    "no-explicit-any": 0,
    "no-var-requires": 0,
    "no-use-before-define": 0,
    "no-unused-vars": [
      2,
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "no-console": [
      2,
      {
        "allow": ["warn", "error"]
      }
    ]
  }
}
