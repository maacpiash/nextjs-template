{
  "name": "with-typescript-eslint-jest",
  "author": {
    "email": "github@maacpiash.com",
    "name": "Ahad Chowdhury",
    "url": "https://www.maacpiash.com"
  },
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "clean": "rimraf out",
    "dev": "next",
    "export": "next export",
    "format": "prettier --write .",
    "lint": "next lint",
    "sb": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "start": "next start",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn format",
      "yarn lint",
      "yarn type-check"
    ]
  },
  "dependencies": {
    "next": "^12.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^6.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.4"
  }
}
