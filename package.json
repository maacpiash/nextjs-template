  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "clean": "rimraf out",
    "dev": "next",
    "export": "next export",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --quiet --fix",
